{
	"[c]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools",
		"editor.formatOnSave": true
	},
	"files.associations": {
		"*.html": "html",
		"*.asm": "asm-intel-x86-generic",
		"*.py": "python",
		"*.c": "C",
		"*.cpp": "cpp",
		"*.h": "C",
		"*.hpp": "cpp",
		"iostream": "cpp"
	},
	"workbench.editorAssociations": {
		"*.c": "default",
		"*.cpp": "default",
		"*.h": "default",
		"*.hpp": "default"
	}
}